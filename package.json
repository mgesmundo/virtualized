  "description": "Efficiently render large lists and tables",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/znck/virtualized.git"
  },
  "keywords": [],
  "author": {
    "name": "Rahul Kadyan",
    "email": "hi@znck.me",
    "url": "https://znck.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/znck/virtualized/issues"
  },
  "homepage": "https://github.com/znck/virtualized#readme",
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@types/jest": "^24.0.1",
    "@types/node": "^11.9.3",
    "any-observable": "^0.3.0",
    "babel-jest": "^24.0.0",
    "husky": "^1.3.0",
    "jest": "^24.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.0",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-typescript2": "^0.19.0",
    "rxjs": "^6.4.0",
    "ts-jest": "^23.0.0",
    "typescript": "^3.3.3",
    "vue": "^2.6.6",
    "vue-jest": "^3.0.3",
    "vue-template-compiler": "^2.6.6"
  },
  "scripts": {
    "test": "jest",
    "pre:build": "rm -rf dist/",
    ":build": "rollup -c --environment BUILD:production",
    "prepublishOnly": "npm run :build",
    "pre:release": "npm run test",
    ":release": "standard-version",
    "post:release": "git push --follow-tags origin master && npm publish",
    "release": "npm run :release"
  },
  "files": [
    "dist/"
  ],
  "browserslist": "> 0.25%, not dead",
  "main": "dist/virtualized.js",
  "module": "dist/virtualized.es.js",
  "browser": "src/index.js",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "vue",
      "json"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest",
      "^.+\\.vue$": "vue-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@znck/prop-types": "^0.5.4",
    "resize-observer-polyfill": "^1.5.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
